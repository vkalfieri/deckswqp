<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Listing</th>
      <th>Description</th>
      <th>Condition</th>
      <th>Card number</th>
      <th>Card name</th>
      <th>Card set</th>
      <th>Category</th>
      <th>Subcategory</th>
      <th>Delivery</th>
      <th>Price</th>
      <th>Trade</th>
      <th>Seller</th>
      <th>User</th>
      <th>Stripe</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.listing_id %></td>
        <td><%= listing.description %></td>
        <td><%= listing.condition_id %></td>
        <td><%= listing.card_number %></td>
        <td><%= listing.card_name %></td>
        <td><%= listing.card_set %></td>
        <td><%= listing.category_id %></td>
        <td><%= listing.subcategory_id %></td>
        <td><%= listing.delivery_id %></td>
        <td><%= listing.price %></td>
        <td><%= listing.trade_id %></td>
        <td><%= listing.seller_id %></td>
        <td><%= listing.user_id %></td>
        <td><%= listing.stripe_id %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
